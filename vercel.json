{
  "routes": [
    { "handle": "filesystem" },                               // 1️⃣ static files first
    {
      "src": "/\\.well-known/farcaster\\.json",                // 2️⃣ exact‐match manifest
      "dest": "/.well-known/farcaster.json"
    },
    {
      "src": "/.*",                                            // 3️⃣ everything else
      "dest": "/index.html"
    }
  ]
}